#include <iostream>
using namespace std;
int main()
{
	int a[15][15];
	int n;
	cin >> n;
	int num;
	for (int i = 0; i < n; i++)
	{
		cin >> num;
		for (int j = 0; j < num; j++)
		{
			a[j][0] = 1;
			for (int l = 1; l < j; l++)
			{
				a[j][l] = a[j - 1][l] + a[j - 1][l - 1];
			}
			a[j][j]=1;
		}
	}
	for (int i = 0; i < num; i++)
	{
		for (int j = 0; j <= i; j++)
		{
			printf("%-2d",a[i][j]);
			cout << " ";
		}
		cout << endl;
	}
}
